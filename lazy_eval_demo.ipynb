   "source": [
    "# Lazy Evaluation in Python\n",
    "\n",
    "This notebook demonstrates lazy evaluation using generators."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def lazy_numbers(n):\n",
    "    for i in range(n):\n",
    "        print(f\"Generating {i}\")\n",
    "        yield i\n",
    "\n",
    "gen = lazy_numbers(5)\n",
    "print(next(gen))\n",
    "print(next(gen))"
   ]
  }
 ],
